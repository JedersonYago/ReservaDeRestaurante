{
  "buildCommand": "npm run build:frontend",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [],
  "env": {
    "VITE_API_URL": "https://reservafacil-production.up.railway.app"
  },
  "functions": {
    "frontend/src/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
